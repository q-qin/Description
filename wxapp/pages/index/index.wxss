/*tips*/

#tips {
  border-top: 1px solid #f0f0f0;
}

#tips navigator {
  width: 33.2%;
  height: 100px;
  float: left;
  text-align: center;
  border-right: 1px solid #f0f0f0;
  background-position: center 33pt;
  background-repeat: no-repeat;
  background-size: contain;
  position: relative;
}

