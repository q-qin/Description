}

/*hunYan*/
#hunYan{
  clear:both;
  border-top: 1px solid #f0f0f0;
  padding-top:5pt;
}
#hunYan .tit{
  width: 97%;
  padding-left:3%;
  font-size: 30rpx;
  position: relative;
  display:block;
}

#hunYan .tit navigator{
  width: auto;
  position: absolute;
  color:#999;
  top:0;
  right: 30rpx;
  font-size: 26rpx;
}

#hunYan navigator {
  width: 33.2%;
  height: auto;
  float: left;
  text-align: center;
  background-position: center 33pt;
  background-repeat: no-repeat;
  background-size: contain;
  position: relative;
}

#hunYan navigator image{
  width:85%;
  height:220rpx;
  display: block;
  padding-top: 7.5%;
  padding-bottom: 20%;
}

#hunYan navigator p{
  position: absolute;
  bottom:5rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  color:#333;
}


