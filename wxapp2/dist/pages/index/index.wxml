    </form>
    <block wx:if='{{!!verflag && !!intConfigs.moreFun.appId}}'>
      <navigator class='moreFun' target="miniProgram" app-id="{{intConfigs.moreFun.appId}}" path="{{intConfigs.moreFun.path}}" open-type="navigate" bindtap='tapRecord' data-appid="{{intConfigs.moreFun.appId}}">更多好玩</navigator>
    </block>
    <block wx:if='{{!!verflag && !!intConfigs.moreAd.appId}}'>
